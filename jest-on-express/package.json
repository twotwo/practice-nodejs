{
  "name": "jest-on-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npx cross-env PORT=8080 CONTEXT=/jest DB_HOSTNAME= DEBUG=srv:* nodemon --trace-sync-io ./bin/www",
    "perf": "npx cross-env PORT=8080 CONTEXT=/jest DEBUG=srv:* node ./bin/www"
  },
  "dependencies": {
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "memory-cache": "^0.2.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.1",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "debug": "^2.6.9",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "config": {
    "validate-commit-msg": {
      "doc": "https://github.com/conventional-changelog-archived-repos/validate-commit-msg",
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      }
    }
  }
}
